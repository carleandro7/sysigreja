<div class="row">
  <div class="col-lg-6 form-group">
    <label class='control-label'><%= t 'activerecord.attributes.user.nome' %></label>
    <input readonly="true" class='form-control' value="<%= @user.nome %>" />
  </div>
  <div class="col-lg-5 form-group">
    <label class='control-label'><%= t 'activerecord.attributes.user.email' %></label>
    <input readonly="true" class='form-control' value="<%= @user.email %>" />
  </div>
  <div class="col-lg-3 form-group">
    <label class='control-label'><%= t 'activerecord.attributes.user.telefone' %></label>
    <input readonly="true" class='form-control' value="<%= @user.telefone %>" />
  </div>
  <div class="col-lg-3 form-group">
    <label class='control-label'><%= t 'activerecord.attributes.user.tipo' %></label>
    <input readonly="true" class='form-control' value="<%=list_tipo_usuario_invertida[@user.tipo] %>" />
  </div>

  <div class="col-lg-5 form-group">
    <label class='control-label'><%= t 'activerecord.attributes.user.igreja' %></label>
    <input readonly="true" class='form-control' value="<%= @user.igreja.nome %>" />
  </div>

</div>


